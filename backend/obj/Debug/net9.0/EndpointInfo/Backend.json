  "components": {
    "schemas": {
      "AITeamAnalysisRequest": {
        "type": "object",
        "properties": {
          "teamProfile": {
            "$ref": "#/components/schemas/TeamProfile"
          },
          "projectContext": {
            "type": "string",
            "nullable": true
          },
          "focusAreas": {
            "type": "array",
