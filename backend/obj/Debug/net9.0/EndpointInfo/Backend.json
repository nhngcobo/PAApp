              "$ref": "#/components/schemas/AnalysisFocus"
            },
            "nullable": true
          },
          "includeDetailedInsights": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "AnalysisFocus": {
        "enum": [
          0,
          1,
          2,
          3,
          4,
          5
        ],
        "type": "integer",
        "format": "int32"
      },
      "MatchRequest": {
        "type": "object",
        "properties": {
          "skills": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "technologies": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "projectType": {
            "type": "string",
            "nullable": true
          },
          "priority": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "StringDoubleKeyValuePair": {
        "type": "object",
        "properties": {
          "key": {
            "type": "string",
            "nullable": true
          },
          "value": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "TeamAnalysisRequest": {
        "type": "object",
        "properties": {
          "employeeIds": {
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int32"
            },
            "nullable": true
          },
          "projectRequirements": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "TeamProfile": {
        "type": "object",
        "properties": {
          "totalMembers": {
            "type": "integer",
            "format": "int32"
          },
          "availableMembers": {
            "type": "integer",
            "format": "int32"
          },
          "skillsBreakdown": {
            "type": "object",
            "additionalProperties": {
              "type": "number",
              "format": "double"
            },
            "nullable": true
          },
          "avgExperience": {
            "type": "number",
            "format": "double"
          },
          "departments": {
            "type": "object",
            "additionalProperties": {
              "type": "integer",
              "format": "int32"
            },
            "nullable": true
          },
          "avgMatchScore": {
            "type": "number",
            "format": "double"
          },
          "availabilityRatio": {
            "type": "number",
            "format": "double",
            "readOnly": true
          },
          "dominantSkill": {
            "$ref": "#/components/schemas/StringDoubleKeyValuePair"
          },
          "skillDiversity": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          }
        },
        "additionalProperties": false
      }
